{
    "extends": "astro/tsconfigs/strictest",
    "compilerOptions": {
        "types": ["astro/client", "google.maps"],
        "baseUrl": ".",
        "paths": {
            "@components/*": ["./src/components/*"],
            "@interfaces/*": ["./src/interfaces/*"],
            "@layouts/*": ["./src/layouts/*"],
            "@pages/*": ["./src/pages/*"],
            "@scripts/*": ["./src/scripts/*"],
            "@stores/*": ["./src/stores/*"],
            "@styles/*": ["./src/styles/*"],
            "@utilities/*": ["./src/utilities/*"]
        }
    },
    "exclude": ["node_modules"]
}